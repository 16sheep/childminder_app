<h1><%= @user.name %></h1>

<h2>Your children</h2>
<ul>
  <% @children.each do |child| %>
    <li><h3><%=link_to child.name, [@user,child] %></h3></li>
    <li><p><%= child.age %></p></li>
  <% end  %>

</ul>

<h2><%= link_to "Your Bookings", user_session_bookings_path(@user) %></h2>

<h2><%= link_to "Your Availabilities", user_availabilities_path(@user) %></h2>
