<div class="container">

    <div class="row">
        <h1 class="col-sm h4 mb-1 font-weight-light">Your Bookings</h1>
        <%= month_calendar events: @booking_times do |date, booking| %>
          <%= date %>
          <% booking.each do |booking| %>
              <%= current_user.children.each do |child| %>
                <div><%= "#{child.name} is booked with #{booking.user.name}"%></div>
              <% end %>
          <% end %>
        <% end %>
    </div>



</div>
